        <Label alignment="CENTER" layoutX="157.0" layoutY="37.0" prefHeight="23.0" prefWidth="326.0" style="&#10;" text="Flight Reservation Tracker" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Label layoutX="289.0" layoutY="77.0" text="Username" />
            <Label layoutX="288.0" layoutY="128.0" text="Password" />
