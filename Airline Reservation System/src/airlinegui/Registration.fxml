            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="tfname" layoutX="51.0" layoutY="23.0" text="First Name" />
            <TextField fx:id="tlname" layoutX="237.0" layoutY="23.0" text="Last Name" />
            <TextField fx:id="taddress" layoutX="51.0" layoutY="69.0" text="Street Address" />
            <TextField fx:id="tstate" layoutX="237.0" layoutY="69.0" text="State" />
            <TextField fx:id="tzip" layoutX="424.0" layoutY="69.0" text="Zip Code" />
            <TextField fx:id="temail" layoutX="51.0" layoutY="115.0" text="Email Address" />
            <TextField fx:id="tssn" layoutX="237.0" layoutY="115.0" text="Social Security Number" />
            <TextField fx:id="tuname" layoutX="51.0" layoutY="159.0" text="Username" />
            <TextField fx:id="tpword" layoutX="237.0" layoutY="159.0" text="Password" />
            <TextField fx:id="tquestion" layoutX="51.0" layoutY="200.0" text="Security Question" />
            <TextField fx:id="tanswer" layoutX="237.0" layoutY="200.0" text="Security Answer" />
            <CheckBox fx:id="admin" layoutX="100.0" layoutY="261.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="70.0" text="Admin" />
            <Button layoutX="203.0" layoutY="317.0" mnemonicParsing="false" onAction="#register" text="Register" />
            <Button layoutX="370.0" layoutY="317.0" mnemonicParsing="false" onAction="#close" text="Cancel" />
         </children>
