<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="airlinegui.RegistrationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="tfname" layoutX="51.0" layoutY="23.0" text="First Name" />
            <TextField fx:id="tlname" layoutX="237.0" layoutY="23.0" text="Last Name" />
            <TextField fx:id="taddress" layoutX="51.0" layoutY="69.0" text="Street Address" />
            <TextField fx:id="tstate" layoutX="237.0" layoutY="69.0" text="State" />
            <TextField fx:id="tzip" layoutX="424.0" layoutY="69.0" text="Zip Code" />
            <TextField fx:id="temail" layoutX="51.0" layoutY="115.0" text="Email Address" />
            <TextField fx:id="tssn" layoutX="237.0" layoutY="115.0" text="Social Security Number" />
            <TextField fx:id="tuname" layoutX="51.0" layoutY="168.0" text="Username" />
            <TextField fx:id="tquestion" layoutX="52.0" layoutY="214.0" text="Security Question" />
            <TextField fx:id="tanswer" layoutX="237.0" layoutY="214.0" text="Security Answer" />
            <CheckBox fx:id="admin" layoutX="100.0" layoutY="261.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="70.0" text="Admin" />
            <Button layoutX="203.0" layoutY="317.0" mnemonicParsing="false" onAction="#register" text="Register" />
            <Button layoutX="370.0" layoutY="317.0" mnemonicParsing="false" onAction="#logout" text="Cancel" />
            <PasswordField fx:id="tpword" layoutX="237.0" layoutY="168.0" />
            <Label layoutX="290.0" layoutY="151.0" text="Password" />
         </children>
    </AnchorPane>
  </children>
</VBox>
